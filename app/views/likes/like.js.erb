$("#micropostLike-<%= @micropost.id %>").html("<%= j render(partial: 'likes/likeForm',locals: {micropost: @micropost});%>");
if ($.trim($('#list-like-<%= @micropost.id %>').html()).length != 0){
	$("#list-like-<%= @micropost.id %>").html("<%= j render(partial: 'likes/likeshow', locals: {users: @likeusers})%>");
}

if ($.trim($('#list-unlike-<%= @micropost.id %>').html()).length != 0){
	$("#list-unlike-<%= @micropost.id %>").html("<%= j render(partial: 'likes/unlikeshow',locals:{users: @unlikeusers})%>");
}